<!doctype html>
<html>
<head>
    <title>Vending Machine: Full Stack App.</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <link rel="stylesheet" href="style.css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.1.0/css/all.css" integrity="sha384-lKuwvrZot6UHsBSfcMvOkWwlCMgc0TaWr+30HWe3a4ltaBwTZhyTEggF5tJv8tbt" crossorigin="anonymous">
</head>
<body>
<section class="container">

    <section class="jumbotron text-center">

        <h1>Vending Machine</h1>
        <a href="/login" class="btn btn-default">Login</a>
        <a href="/signup" class="btn btn-default">Signup</a>

        <section class="vendingContainer">
          <span>$1 sodas</span>
          <section class="itemBox">
            <section class="displayItem">
              <h2>Coke</h2>
              <span>1</span>
            </section>
            <section class="displayItem">
              <h2>Sprite</h2>
              <span>2</span>
            </section>
            <section class="displayItem">
              <h2>Fanta</h2>
              <span>3</span>
            </section>
            <section class="displayItem">
              <h2>Dr. P</h2>
              <span>4</span>
            </section>
            <section class="displayItem">
              <h2>Pepsi</h2>
              <span>5</span>
            </section>
          </section>

          <section class="buttonContainer">
            <button class="item" type="button" value="coke" name="button">1</button>
            <button class="item" type="button" value="sprite" name="button">2</button>
            <button class="item" type="button" value="fanta" name="button">3</button>
            <button class="item" type="button" value="drPepper" name="button">4</button>
            <button class="item" type="button" value="pepsi" name="button">5</button>
            <button id="submit" type="button" name="button">submit</button>
          </section>

        </section>



    </section>
    <script type="text/javascript" src="main.js">

    </script>
</section>
</body>
</html>
